{
	"name": "alfred-icons",
	"version": "0.0.4",
	"description": "Quick search, preview directly in alfred any icons by the thenounproject.com service.",
	"license": "MIT",
	"repository": "bikenik/alfred-icons",
	"author": {
		"name": "bikenik",
		"email": "bikeniks@gmail.com",
		"url": "http://bikenik.org"
	},
	"engines": {
